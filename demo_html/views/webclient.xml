               <script type="text/javascript" src="/demo_html/static/src/js/menu.js"></script>
               <!--左侧菜单-->
                <div class="i_yuan_man_menu">
                   <ul class="menu_list_view">
                       <li class="menu_list">菜单1</li>
                       <li class="menu_list">菜单2</li>
                       <li class="menu_list">菜单3</li>
                   </ul>
                </div>
