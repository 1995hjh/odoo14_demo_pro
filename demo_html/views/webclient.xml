<odoo>
    <data>
        <template id="iym_assets_backend_name" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <link rel="stylesheet" type="text/scss" href="/demo_html/static/src/scss/menu.scss"/>
            </xpath>
        </template>
        <template id="i_yuan_man_menu" inherit_id="web.webclient_bootstrap">
           <xpath expr="//t[@t-call='web.layout']" position="inside">
